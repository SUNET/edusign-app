        <section id="body">
          <h1 id="main-title">{{ gettext("eduSign - Secure digital signature") }}</h1>
          <div id="login"></div>
          <div id="md-content">
            {{ body | markdown }}
          </div>
        </section>
        <footer>
          <div id="app-version">
            {{ version }}
          </div>
          <div id="lang-selector">
            {{ other_lang_name }}
          </div>
        </footer>
      </div>
    </div>
    <script>
        window.onload = function() {
          thiss.DiscoveryComponent({ loginInitiatorURL: '{{ login_initiator }}' }).render('#login');

          document.querySelector('#lang-selector').addEventListener('click', (e) => {
            e.preventDefault();
            var days = 365;
            var date = new Date();
            date.setTime(date.getTime() + (days*24*60*60*1000));
            var cookie = "lang={{ other_lang }}; expires=" + date.toUTCString();
            window.document.cookie = cookie;
            window.location.reload(true);
          });
        };
    </script>
  </body>
